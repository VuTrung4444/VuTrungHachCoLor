                                    game:GetService("ReplicatedStorage").Remotes.To_Server.Handle_Initiate_S:FireServer("arrow_knock_back_damage", game.Players.LocalPlayer.Character, targetCFrame, character, 99999, 99999)
                                    task.wait(0.2)
                                end
                                task.wait(1)
                            end
                        end
                    end
                end
            end
        end
    end
end)



