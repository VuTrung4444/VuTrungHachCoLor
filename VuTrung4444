    while true do
        wait(600) -- Chờ 10 phút (600 giây)
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new()) -- Giả lập nhấp chuột phải để reset AFK
    end
end)


---Nhặt Hoa---
local TweenService = game:GetService("TweenService")
local flowers = game:GetService("Workspace").Demon_Flowers_Spawn
local Player = game.Players.LocalPlayer

local function fireproximityprompt(ProximityPrompt, Amount, Skip)
    local HoldDuration = ProximityPrompt.HoldDuration
    if Skip then
        ProximityPrompt.HoldDuration = 0
    end

    for i = 1, Amount or 1 do
        ProximityPrompt:InputHoldBegin()
        task.wait(HoldDuration)
        ProximityPrompt:InputHoldEnd()
    end

    ProximityPrompt.HoldDuration = HoldDuration
end

local function TweenToPosition(TargetPosition, Speed)
    local HRP = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")
    if not HRP then return end

    local distance = (HRP.Position - TargetPosition).magnitude
    local duration = distance / Speed

    local TweenInfoData = TweenInfo.new(duration, Enum.EasingStyle.Linear)
    local Tween = TweenService:Create(HRP, TweenInfoData, {CFrame = CFrame.new(TargetPosition)})
    Tween:Play()

    return Tween, duration
end

local function getFlower()
    local chr = Player.Character
    local root = chr and chr:FindFirstChild("HumanoidRootPart")
    if not root then return nil end

    local dist, flower = math.huge, nil
    for _, v in next, flowers:GetChildren() do
        if v:IsA('Model') then
            local mag = (root.Position - v.WorldPivot.Position).magnitude
            if mag < dist then
                dist = mag
                flower = v
            end
        end
    end
    return flower
end

spawn(function()
    while task.wait(1) do
        if AutoCollectLilys then
            local attempts = 0
            while AutoCollectLilys do
                task.wait(0.2) -- Giảm thời gian chờ tìm hoa

                local flower = getFlower()

                if not flower then
                    attempts = attempts + 1
                    if attempts >= 5 then
                        warn("⚠️ Không tìm thấy hoa sau 5 lần thử.")
                        task.wait(2) -- Nghỉ trước khi thử lại để giảm tải
                    end
                    continue
                end

                if flower.WorldPivot then
                    local tween, duration = TweenToPosition(flower.WorldPivot.Position, 250)

                    local maxTime = duration + 1 -- 1 giây dự phòng cho nhặt hoa
                    local startTime = tick()

                    repeat
                        task.wait()
                    until not tween.PlaybackState == Enum.PlaybackState.Playing or (tick() - startTime > maxTime)

                    if tween.PlaybackState == Enum.PlaybackState.Playing then
                        tween:Cancel()
                    end

                    -- Khi đã đến hoa, tăng tốc độ nhặt nhanh hơn
                    local collected = false
                    for _, v in next, flower:GetDescendants() do
                        if v:IsA("ProximityPrompt") then
                            fireproximityprompt(v, 1, true)
                            collected = true
                        end
                    end

                    -- Theo dõi sự biến mất của hoa nhanh hơn
                    local waitTime = tick()
                    while tick() - waitTime < 2 do -- Chỉ chờ tối đa 2 giây để check hoa biến mất
                        task.wait(0.1) -- Kiểm tra nhanh hơn
                        if not flower.Parent then
                            break
                        end
                    end

                    if flower.Parent then
                        warn("⚠️ Không thể nhặt hoa hoặc hoa không biến mất!")
                    end
                else
                    warn("⚠️ WorldPivot của hoa không tồn tại!")
                end

                attempts = 0
            end
        end
    end
end)

