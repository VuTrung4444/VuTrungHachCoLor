Teleports:AddButton({
    Name = "Skibidi To",
    Callback = function()
        local Player = game.Players.LocalPlayer
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")
        
        if not HumanoidRootPart then
            warn("‚ö†Ô∏è L·ªói: Kh√¥ng t√¨m th·∫•y HumanoidRootPart!")
            return
        end

        local TweenService = game:GetService("TweenService")
        local TweenSpeed = 300 -- T·ªëc ƒë·ªô di chuy·ªÉn

        -- Ki·ªÉm tra SelectedArea h·ª£p l·ªá
        if not SelectedArea or SelectedArea == "" then
            warn("‚ö†Ô∏è L·ªói: Ch∆∞a ch·ªçn khu v·ª±c ƒë·ªÉ d·ªãch chuy·ªÉn!")
            return
        end

        -- X√°c ƒë·ªãnh MapToUse d·ª±a tr√™n Map ƒë√£ ch·ªçn
        local MapToUse = {}
        if SelectedMap == "Map1" then
            MapToUse = Map1
        elseif SelectedMap == "Map2" then
            MapToUse = Map2
        else
            warn("‚ö†Ô∏è L·ªói: Ch∆∞a ch·ªçn Map h·ª£p l·ªá!")
            return
        end

        -- Ki·ªÉm tra Place h·ª£p l·ªá
        local Place = MapToUse[SelectedArea]
        if not Place then
            warn("‚ö†Ô∏è L·ªói: Kh√¥ng t√¨m th·∫•y t·ªça ƒë·ªô c·ªßa khu v·ª±c [" .. SelectedArea .. "] trong Map [" .. SelectedMap .. "]!")
            return
        end

        -- T√≠nh kho·∫£ng c√°ch
        local Magnitude = (HumanoidRootPart.Position - Place.Position).Magnitude
        print("üìç Kho·∫£ng c√°ch ƒë·∫øn m·ª•c ti√™u:", Magnitude)

        -- B·∫Øt ƒë·∫ßu Tween ƒë·ªÉ di chuy·ªÉn
        Teleporting = true
        local TargetCFrame = Place * CFrame.new(0, 5, 0) -- N√¢ng cao nh√¢n v·∫≠t 5 ƒë∆°n v·ªã ƒë·ªÉ tr√°nh k·∫πt

        local Cur = TweenService:Create(HumanoidRootPart, TweenInfo.new(Magnitude / TweenSpeed, Enum.EasingStyle.Linear), {CFrame = TargetCFrame})
        Cur:Play()
        Cur.Completed:Wait()

        Teleporting = false
        print("‚úÖ D·ªãch chuy·ªÉn th√†nh c√¥ng!")
    end
})

