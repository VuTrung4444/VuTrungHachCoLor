    Name = "No Fog",
    Flag = "NF",
    Keybind = false,
    Callback = function (v)
        if v == true then
            for index, light in pairs(game:GetService("Lighting"):GetChildren()) do
                if light.Name == "Atmosphere" or string.find(light.Name, "Blur") or string.find(light.Name, "Bloom") then
                    light:Destroy()
                end
            end
            game:GetService("Lighting").FogEnd = 1000000
            elseif v == false then
            print("1")
        end
    end
})

