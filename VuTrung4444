        huntLegendary = state
    end,
    State = false  
})

ClanChooseSection:AddToggle({
    Name = "Săn Mythic Clan",
    Flag = "HuntMythic",
    Callback = function(state)
        huntMythic = state
    end,
    State = false  
})

ClanChooseSection:AddToggle({
    Name = "Săn Supreme Clan",
    Flag = "HuntSupreme",
    Callback = function(state)
        huntSupreme = state
    end,
    State = true
})
end
