--GodModeState--
local godModeToggle = GodModeQuick:AddToggle({
    Name = "Enable God Mode",
    Flag = "ENG_Toggle",
    Callback = function(v)
        godModeState = v
        ENG = v
    end
})

local function toggleGodMode(state)
    godModeState = state
    ENG = state

    -- Cập nhật giao diện của toggle
    if godModeToggle.Callback then
        task.spawn(function()
            godModeToggle.Callback(state)
			print("test")
        end)
    end
end

GodModeQuick:AddButton({
    Name = "Enable God Mode",
    Callback = function()
        toggleGodMode(not godModeState) -- Đảo trạng thái bật/tắt
    end
})

for _, godMode in ipairs(GodModes) do
    local toggle = GodModeQuick:AddToggle({
        Name = godMode,
        Flag = "Gm_" .. godMode, -- Thêm Flag để quản lý dễ dàng hơn nếu cần
        Callback = function(state)
            if state then
                -- Tắt toggle trước đó nếu có và khác với toggle hiện tại
                if activeToggle and activeToggle ~= toggle then
                    activeToggle:SetState(false)
                end
                -- Cập nhật toggle hiện tại và thiết lập GodSelected
                activeToggle = toggle
                getgenv().GodSelected = godMode
                print("God Mode Selected: " .. godMode)
            else
                -- Chỉ clear nếu chính toggle hiện tại bị tắt
                if activeToggle == toggle then
                    activeToggle = nil
                    getgenv().GodSelected = nil
                    print("God Mode Deselected")
                end
            end
        end
    })

    -- Lưu trữ toggle để có thể kiểm soát sau này nếu cần
    toggles[godMode] = toggle
end


